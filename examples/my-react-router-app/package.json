{
	"name": "my-react-router-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm run build:react && pnpm run build:lambda",
		"build:react": "dotenv -e ../../.envrc -- react-router build",
		"build:lambda": "dotenv -e ../../.envrc -- node build.cjs",
		"dev": "dotenv -e ../../.envrc -- react-router dev",
		"start": "dotenv -e ../../.envrc -- react-router-serve ./build/server/index.js",
		"typecheck": "react-router typegen && tsc"
	},
	"dependencies": {
		"@ballatech/react-router7-preset-aws": "workspace:*",
		"@react-router/node": "^7.8.2",
		"@react-router/serve": "^7.8.2",
		"isbot": "^5.1.30",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.8.2"
	},
	"devDependencies": {
		"@react-router/dev": "^7.8.2",
		"@tailwindcss/vite": "^4.1.12",
		"@types/node": "^20.19.11",
		"@types/react": "^19.1.12",
		"@types/react-dom": "^19.1.9",
		"dotenv-cli": "catalog:",
		"esbuild": "catalog:",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"vite": "^6.3.5",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
